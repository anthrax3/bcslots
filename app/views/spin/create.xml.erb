<?xml version="1.0" encoding="UTF-8"?>
<data
  <% if @data[:error] %>
    error="<%= @data[:error] %>"
  <% end %>
  <% if @data[:payout] %>
    payout="<%= @data[:payout] %>"
  <% end %>
  <% if @data[:reels] %>
    reel1="<%= @data[:reels][0] %>"
    reel2="<%= @data[:reels][1] %>"
    reel3="<%= @data[:reels][2] %>"
  <% end %>
  <% if @data[:balance] %>
    balance="<%= @data[:balance] %>"
  <% end %>
  <% if @data[:balance_in_dBTC] %>
    balance_in_dBTC="<%= @data[:balance_in_dBTC] %>"
  <% end %>
  <% if @data[:balance_in_cBTC] %>
    balance_in_cBTC="<%= @data[:balance_in_cBTC] %>"
  <% end %>
  <% if @data[:balance_in_mBTC] %>
    balance_in_mBTC="<%= @data[:balance_in_mBTC] %>"
  <% end %>
  <% if @data[:previous_balance] %>
    previous_balance="<%= @data[:previous_balance] %>"
  <% end %>
  <% if @data[:balance_before_payout_in_dBTC] %>
    balance_before_payout_in_dBTC="<%= @data[:balance_before_payout_in_dBTC] %>"
  <% end %>
  <% if @data[:balance_before_payout_in_cBTC] %>
    balance_before_payout_in_cBTC="<%= @data[:balance_before_payout_in_cBTC] %>"
  <% end %>
  <% if @data[:balance_before_payout_in_mBTC] %>
    balance_before_payout_in_mBTC="<%= @data[:balance_before_payout_in_mBTC] %>"
  <% end %>
  <% if @data[:next_hash] %>
    next_hash="<%= @data[:next_hash] %>"
  <% end %>
  <% if @data[:secret] %>
    next_hash="<%= @data[:secret] %>"
  <% end %>
  <% if @data[:position] %>
    next_hash="<%= @data[:position] %>"
  <% end %>
  >
</data>
